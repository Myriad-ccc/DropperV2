<Application
    x:Class="DropperV2.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:DropperV2"
    StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="ClosingColor">#9c150e</SolidColorBrush>
        <SolidColorBrush x:Key="MinimizeColor">#1566bd</SolidColorBrush>

        <SolidColorBrush x:Key="TileEmpty" Color="#141414" />
        
        <SolidColorBrush x:Key="TilePlayer" Color="#305CDE" />
        <SolidColorBrush x:Key="TilePlayerBorder" Color="#6495ED" />
        <SolidColorBrush x:Key="TileEnemy" Color="#DE3030" />
        <SolidColorBrush x:Key="TileEnemyBorder" Color="#ED6473" />

        <SolidColorBrush x:Key="placeholder" Color="#FF3333" />
        <SolidColorBrush x:Key="TileCoin" Color="#FFFF33" />
        <SolidColorBrush x:Key="TileWall" Color="#808080" />
    </Application.Resources>
</Application>
