<Application x:Class="DropperV2.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DropperV2"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="ClosingColor">#9c150e</SolidColorBrush>
        <SolidColorBrush x:Key="MinimizeColor">#1566bd</SolidColorBrush>

        <SolidColorBrush x:Key="TileBorder" Color="#282828"/>
        <SolidColorBrush x:Key="Tile.Empty" Color="#141414" />
        <SolidColorBrush x:Key="Tile.Player" Color="#33FF33" />
        <SolidColorBrush x:Key="Tile.Enemy" Color="#FF3333" />
        <SolidColorBrush x:Key="Tile.Coin" Color="#FFFF33" />
        <SolidColorBrush x:Key="Tile.Wall" Color="#808080" />
    </Application.Resources>
</Application>
